{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": false,
    "interface-name": [true, "never-prefix"],
    "import-spacing": false,
    "ban-types": false,
    "no-bitwise": false,
    "curly": false,
    "member-ordering": false,
    "max-classes-per-file": false,
    "quotemark": {
      "options": [
        "single",
        "avoid-escape"
      ]
    },
    "typedef-whitespace": {
      "options": [{
        "property-declaration": false
      }]
    }
  },
  "rulesDirectory": []
}
